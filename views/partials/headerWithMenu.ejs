                </label>
                </form>
            <% }else { %>
              <% console.log(userPrefs, 'else') %>
              <form action="/theme/dark" method="POST">
                <label class="swap swap-rotate">
                  <input type="checkbox" class="theme-controller" onchange="this.form.submit()" value=""/>
